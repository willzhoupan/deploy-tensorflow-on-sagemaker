    "import tarfile\n",
    "import os\n",
    "#create file name\n",
    "tar = tarfile.open(\"./sagemaker.tar.gz\",\"w:gz\")\n",
    "#read and compress files\n",
    "for root,dir,files in os.walk(\"./\"):\n",
    "    for file in files:\n",
    "        fullpath = os.path.join(root,file)\n",
    "        tar.add(fullpath)\n",
    "tar.close()\n",
    "print('Done')"
   ]
